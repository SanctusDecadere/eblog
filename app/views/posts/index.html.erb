


<!DOCTYPE html>
<head>
	<meta charset="UTF-8">
	<title>Post Index Page</title>
	<!-- <style src="../../assets/stylesheets/posts.scss"></style> -->
</head>
<body>


<div class="posts-container">
	<% @posts.each do |post| %>
		<div class="post-wrapper">
			<h2 class="post-title"><%= link_to post.title, post %></h2>
			<p class="date"><%= post.created_at.strftime("%B - %d - %Y") %></p>
		</div>
	<% end %>
</div>

	
</body>
</html>




